<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lkd.bt.spider.mapper.MetadataMapper">
    <select id="getInfoHashByPage" resultType="java.lang.String">
        SELECT info_hash
        FROM metadata
        LIMIT #{start}, #{end}
    </select>

</mapper>
